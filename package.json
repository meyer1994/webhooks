{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "clean": "rm -rf .output .wrangler .nuxt",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:generate": "dotenv -e .env -- drizzle-kit generate",
    "cf:deploy": "pnpm run build && wrangler --cwd .output deploy",
    "cf:types": "wrangler types shared/wrangler.d.ts",
    "cf:dev": "wrangler dev",
    "cf:migrate": "wrangler d1 migrations apply flower"
  },
  "dependencies": {
    "@nuxt/eslint": "1.9.0",
    "@nuxt/ui": "^4.0.1",
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.47.0",
    "@vue-flow/minimap": "^1.5.4",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "drizzle-orm": "^0.44.6",
    "nuxt": "^4.1.3",
    "trpc-nuxt": "^1.2.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.18.8",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.37.0",
    "nitro-cloudflare-dev": "^0.2.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.1",
    "wrangler": "^4.42.1"
  }
}